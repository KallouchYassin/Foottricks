<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Foottricks" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/yellow</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/brown</item>
        <item name="android:fontFamily">@font/gilroy_light</item>
        <item name="fontFamily">@font/gilroy_light</item>
     <!-- target android sdk versions < 26 and > 14 if theme other than AppCompat -->

    </style>

    <style name="Theme.Foottricks.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Foottricks.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Foottricks.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

<style name="Dialoge">
    <item name="android:windowBackground">@drawable/empty_back</item>
    <item name="windowNoTitle">true</item>
    <item name="windowMinWidthMajor">80%</item>
    <item name="windowActionBar">false</item>

</style>

</resources>